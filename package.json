    "jest-preset-angular": "^14.5.3",
    "lint-staged": "^13.1.2",
    "miragejs": "^0.1.47",
    "ng-mocks": "^14.11.0",
    "postcss": "^8.4.21",
    "prettier": "3.2.4",
